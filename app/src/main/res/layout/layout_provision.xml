<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ScrollView
        android:id="@+id/scrollView6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/provision_cancel_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <android.support.constraint.ConstraintLayout
                android:id="@+id/provision_constraintLayout2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                app:layout_constraintEnd_toEndOf="@+id/textView43"
                app:layout_constraintTop_toBottomOf="@+id/textView43"
                >

                <ImageView
                    android:id="@+id/provison_check_image2"
                    android:layout_width="12.5dp"
                    android:layout_height="12.5dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/check_off" />

                <TextView
                    android:id="@+id/textView44"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:fontFamily="@font/nanumsquarebold"
                    android:text="동의합니다"
                    android:textColor="#595757"
                    android:textSize="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/provison_check_image2"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:id="@+id/provision_constraintLayout1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toTopOf="@+id/textView43"
                app:layout_constraintEnd_toEndOf="@+id/textView42"
                app:layout_constraintTop_toBottomOf="@+id/textView42">

                <ImageView
                    android:id="@+id/provison_check_image1"
                    android:layout_width="12.5dp"
                    android:layout_height="12.5dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/check_off" />

                <TextView
                    android:id="@+id/textView41"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:fontFamily="@font/nanumsquarebold"
                    android:text="동의합니다"
                    android:textColor="#595757"
                    android:textSize="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/provison_check_image1"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <TextView
                android:id="@+id/textView42"
                android:layout_width="336.5dp"
                android:layout_height="210dp"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="22.5dp"
                android:background="#FFFFFF"
                android:fontFamily="@font/nanumsquareregular"
                android:scrollbars="vertical"
                android:text="어부바 이용약관\n1. 총칙\n제1조 (목적)\n이 약관은 주식화사\n\n\n"
                android:textColor="#000000"
                android:textSize="9dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView43"
                android:layout_width="336.5dp"
                android:layout_height="210dp"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="31dp"
                android:layout_marginBottom="51.5dp"
                android:background="#FFFFFF"
                android:fontFamily="@font/nanumsquareregular"
                android:scrollbars="vertical"
                android:text="TextView\n야간ㄴ\n야간ㄴ\n야간ㄴ\n야간ㄴ\n야간ㄴ\n야간ㄴ\n야간ㄴ\n야간ㄴ\n야간ㄴ\n야간ㄴ\n야간ㅇㄴㄴ\n야간ㄴ\n야간ㄴ"
                android:textColor="#000000"
                android:textSize="9dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView42"
                app:layout_goneMarginBottom="51.5dp" />


        </android.support.constraint.ConstraintLayout>
    </ScrollView>

    <Button
        android:id="@+id/provision_cancel_button"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:background="#595757"
        android:fontFamily="@font/nanumsquarebold"
        android:onClick="provision_ClickLeft"
        android:text="취소"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/provision_next_button"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:background="#E73828"
        android:fontFamily="@font/nanumsquarebold"
        android:onClick="provision_ClickRight"
        android:text="전체동의"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/provision_cancel_button" />
</android.support.constraint.ConstraintLayout>