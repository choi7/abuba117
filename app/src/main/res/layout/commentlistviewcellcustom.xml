<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="62.5dp">

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="17.5dp"
        android:layout_height="18.5dp"
        android:layout_marginStart="14.5dp"
        android:layout_marginTop="8.5dp"
        android:fontFamily="@font/nanumsquareregular"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ch_on"
        android:layout_marginLeft="14.5dp" />

    <TextView
        android:id="@+id/textView83"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="14.5dp"
        android:text="김원장"
        android:textSize="8.75dp"
        android:fontFamily="@font/nanumsquarebold"
        app:layout_constraintStart_toEndOf="@+id/imageView17"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="5dp" />

    <TextView
        android:id="@+id/textView91"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquareregular"
        android:text="네 알겟습니다"
        android:textSize="10dp"
        app:layout_constraintBottom_toTopOf="@+id/textView92"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toBottomOf="@+id/imageView17" />

    <TextView
        android:id="@+id/textView92"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="2018.01.01 오후 11:00"
        android:fontFamily="@font/nanumsquareregular"
        android:textSize="6.25dp"
        android:textColor="#B4B4B5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView17" />
</android.support.constraint.ConstraintLayout>