<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.joyblock.abuba.testActivity"
    tools:layout_editor_absoluteY="81dp">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="78.5dp"
        android:layout_height="78.5dp"
        android:layout_marginStart="22.5dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profile" />

    <TextView
        android:id="@+id/mainSelectschoolText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14.5dp"
        android:layout_marginTop="20.75dp"
        android:text="어부바 유치원"
        android:textSize="22.5dp"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        android:layout_marginLeft="14.5dp" />

    <TextView
        android:id="@+id/mainSelectNameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="김솜솜 원장님"
        android:textSize="11.25dp"
        app:layout_constraintStart_toStartOf="@+id/mainSelectschoolText"
        app:layout_constraintTop_toBottomOf="@+id/mainSelectschoolText" />

    <ListView
        android:id="@+id/listView111"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="16dp"
        android:drawSelectorOnTop="true"
        app:layout_constraintBottom_toTopOf="@+id/mainselectbottombar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <com.joyblock.abuba.bottomtabbar
        android:id="@+id/mainselectbottombar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_weight="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"></com.joyblock.abuba.bottomtabbar>

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginBottom="42.75dp"
        android:layout_marginEnd="12.5dp"
        android:layout_marginTop="42.75dp"
        android:background="#FFFFFF"
        android:src="@mipmap/ic_setting"
        app:layout_constraintBottom_toTopOf="@+id/listView111"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/setting"
        android:layout_marginRight="12.5dp" />

</android.support.constraint.ConstraintLayout>
