<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EEEEEF">


    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout7"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="#EEEEEF"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:background="#EEEEEF"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <TextView
                android:id="@+id/tv_date2"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#EEEEEF"
                android:fontFamily="@font/nanumsquarebold"
                android:gravity="center"
                android:text="일"
                android:textColor="#888888"
                android:textSize="11dp" />

            <TextView
                android:layout_width="40.5dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#EEEEEF"
                android:fontFamily="@font/nanumsquarebold"
                android:gravity="center"
                android:text="요일"
                android:textColor="#888888"
                android:textSize="11dp" />

            <TextView
                android:layout_width="42.75dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#EEEEEF"
                android:fontFamily="@font/nanumsquarebold"
                android:gravity="center"
                android:text="시간"
                android:textColor="#888888"
                android:textSize="11dp" />

            <TextView
                android:layout_width="82.5dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#EEEEEF"
                android:fontFamily="@font/nanumsquarebold"
                android:gravity="center"
                android:text="장소"
                android:textColor="#888888"
                android:textSize="11dp" />

            <TextView
                android:layout_width="97.5dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#EEEEEF"
                android:fontFamily="@font/nanumsquarebold"
                android:gravity="center"
                android:text="제목"
                android:textColor="#888888"
                android:textSize="11dp" />

            <TextView
                android:layout_width="56.5dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#EEEEEF"
                android:fontFamily="@font/nanumsquarebold"
                android:gravity="center"
                android:text="확인"
                android:textColor="#888888"
                android:textSize="11dp" />

        </LinearLayout>

        <TextView
            android:id="@+id/tv_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/nanumsquareregular"
            android:text="2018.01"
            android:textColor="#000000"
            android:textSize="15.75dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="12.5dp"
            android:layout_height="5.75dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_date"
            app:layout_constraintStart_toStartOf="@+id/tv_date"
            app:layout_constraintTop_toBottomOf="@+id/tv_date"
            app:srcCompat="@drawable/a_b" />

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="14.5dp"
            android:layout_height="14.5dp"
            android:layout_marginEnd="11.25dp"
            android:layout_marginTop="11.25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/listview"
            android:layout_marginRight="11.25dp" />

    </android.support.constraint.ConstraintLayout>

    <ListView
        android:id="@+id/listView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout7" />

</android.support.constraint.ConstraintLayout>