<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:gravity="center">

    <TextView
        android:id="@+id/listview_text1"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:fontFamily="@font/nanumsquarebold"
        android:gravity="center"
        android:paddingLeft="23.25dp"
        android:text="text1"
        android:textSize="13.75dp" />

    <TextView
        android:id="@+id/listview_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/listview_text1"
        android:layout_toRightOf="@+id/listview_text1"
        android:layout_toLeftOf="@+id/listview_text4"
        android:layout_toStartOf="@+id/listview_text4"
        android:layout_weight="1"
        android:fontFamily="@font/nanumsquarebold"
        android:gravity="left|center"
        android:paddingLeft="57dp"
        android:paddingTop="10dp"
        android:text="text2"
        android:textSize="13.75dp" />

    <TextView
        android:id="@+id/listview_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/listview_text1"
        android:layout_toLeftOf="@+id/listview_text4"
        android:layout_toRightOf="@+id/listview_text1"
        android:layout_toStartOf="@+id/listview_text4"
        android:layout_weight="1"
        android:fontFamily="@font/nanumsquarebold"
        android:gravity="left|center"
        android:paddingBottom="10dp"
        android:paddingLeft="57dp"
        android:text="text3"
        android:textSize="8.75dp" />

    <TextView
        android:id="@+id/listview_text4"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="false"
        android:layout_weight="1"
        android:fontFamily="@font/nanumsquarebold"
        android:gravity="center"
        android:paddingRight="23.25dp"
        android:textSize="13.75dp" />

</RelativeLayout>
