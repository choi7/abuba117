    <LinearLayout
        android:id="@+id/linear_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView130"
            android:layout_width="match_parent"
            android:layout_height="43.50dp"
            android:background="#8dc21f"
            android:gravity="center_vertical"
            android:paddingLeft="38dp"
            android:text="등 원"
            android:textColor="#ffffff"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/a3_1ClassConstraintLayout" />

        <LinearLayout
            android:id="@+id/layout_bus_name_to_shcool"
            android:layout_width="match_parent"
            android:layout_height="43.50dp"
            android:gravity="center_vertical"
            android:onClick="changeBusToSchool"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview_bus_name_to_school"
                android:layout_width="match_parent"
                android:layout_height="43.50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="38dp"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/a3_1ClassConstraintLayout"
                tools:text="곰돌이 1호차" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right"
                android:orientation="vertical"
                android:paddingRight="19dp">

                <ImageView
                    android:id="@+id/imageView21"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/document_next_image" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout_point_to_shchool"
            android:layout_width="match_parent"
            android:layout_height="43.50dp"
            android:gravity="center_vertical"
            android:onClick="changePointToSchool"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview_point_to_shchool"
                android:layout_width="match_parent"
                android:layout_height="43.50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="38dp"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/a3_1ClassConstraintLayout"
                tools:text="자이아파트 정문" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right"
                android:orientation="vertical"
                android:paddingRight="19dp">

                <ImageView
                    android:id="@+id/imageView211"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/document_next_image" />
            </LinearLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/textView131"
            android:layout_width="match_parent"
            android:layout_height="43.50dp"
            android:background="#f7b52c"
            android:gravity="center_vertical"
            android:paddingLeft="38dp"
            android:text="하 원"
            android:textColor="#ffffff"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/a3_1ClassConstraintLayout" />

        <LinearLayout
            android:id="@+id/layout__bus_name_to_home"
            android:layout_width="match_parent"
            android:layout_height="45.5dp"
            android:gravity="center_vertical"
            android:onClick="changeBusToHome"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview_bus_name_to_home"
                android:layout_width="match_parent"
                android:layout_height="43.50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="38dp"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/a3_1ClassConstraintLayout"
                tools:text="곰돌이 1호차" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right"
                android:orientation="vertical"
                android:paddingRight="19dp">

                <ImageView
                    android:id="@+id/imageView2111"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/document_next_image" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout_point_to_home"
            android:layout_width="match_parent"
            android:layout_height="43.5dp"
            android:gravity="center_vertical"
            android:onClick="changePointToHome"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textview_point_to_home"
                android:layout_width="match_parent"
                android:layout_height="43.50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="38dp"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/a3_1ClassConstraintLayout"
                tools:text="자이아파트 정문" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right"
                android:orientation="vertical"
                android:paddingRight="19dp">

                <ImageView
                    android:id="@+id/imageView211111"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/document_next_image" />
            </LinearLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/textView134"
            android:layout_width="match_parent"
            android:layout_height="43.50dp"
            android:background="#2ca6e0"
            android:gravity="center_vertical"
            android:paddingLeft="38dp"
            android:text="추 가"
            android:textColor="#ffffff"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/a3_1ClassConstraintLayout" />

        <ListView
            android:id="@+id/listview_another_boarding"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FFFFFF"
            app:layout_constraintBottom_toTopOf="@+id/PageNavigationView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView94"
            app:layout_constraintVertical_bias="0.0">

        </ListView>
    </LinearLayout>

    <com.joyblock.abuba.bottomtabbar
        android:id="@+id/PageNavigationView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_weight="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent">

    </com.joyblock.abuba.bottomtabbar>

</android.support.constraint.ConstraintLayout>