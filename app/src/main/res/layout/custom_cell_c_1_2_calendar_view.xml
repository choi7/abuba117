<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="47.5dp">

    <TextView
        android:id="@+id/listview_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="17.75dp"
        android:layout_marginTop="8dp"
        android:textSize="10dp"
        android:fontFamily="@font/nanumsquareregular"
        android:textColor="#000000"
        android:text="5일"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="17.75dp" />

    <TextView
        android:id="@+id/listview_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="8dp"
        android:textSize="10dp"
        android:fontFamily="@font/nanumsquareregular"
        android:textColor="#000000"
        android:text="금요일"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/listview_text1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/listview_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="8dp"
        android:textSize="10dp"
        android:fontFamily="@font/nanumsquareregular"
        android:textColor="#000000"
        android:text="10시~\n  12시"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/listview_text2"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="14dp" />

    <TextView
        android:id="@+id/listview_text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="21.25dp"
        android:layout_marginTop="8dp"
        android:textSize="10dp"
        android:fontFamily="@font/nanumsquareregular"
        android:textColor="#000000"
        android:text="창원시\n마산회원구"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/listview_text3"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="21.25dp" />

    <TextView
        android:id="@+id/listview_text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="35.5dp"
        android:layout_marginStart="31.5dp"
        android:fontFamily="@font/nanumsquareregular"
        android:text="동계장기자랑"
        android:textColor="#000000"
        android:textSize="10dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView23"
        app:layout_constraintStart_toEndOf="@+id/listview_text4"
        tools:layout_editor_absoluteY="15dp" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="32dp"
        android:layout_height="18dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/save_image" />
</android.support.constraint.ConstraintLayout>