<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="43.5dp">

    <TextView
        android:id="@+id/listview_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7.5dp"
        android:layout_marginStart="23.25dp"
        android:fontFamily="@font/nanumsquarebold"
        android:text="listview_text1"
        android:textColor="#33CCCC"
        android:textSize="11.25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/listview_text2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="2dp"
        android:fontFamily="@font/nanumsquareregular"
        android:text="listview_text2"
        android:textColor="#595757"
        android:textSize="7.5dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView29"
        app:layout_constraintStart_toStartOf="@+id/listview_text1"
        app:layout_constraintTop_toBottomOf="@+id/listview_text1" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="6.25dp"
        android:layout_height="13.25dp"
        android:layout_marginEnd="23.25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/school_register_right" />

</android.support.constraint.ConstraintLayout>