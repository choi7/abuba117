<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="50dp">

    <ImageView
        android:id="@+id/imageview_tel"
        android:layout_width="28.5dp"
        android:layout_height="28.5dp"
        app:srcCompat="@drawable/boarding_list_tel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="23.75dp" />

    <TextView
        android:id="@+id/listview_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23.75dp"
        android:fontFamily="@font/nanumsquarebold"
        android:text="김철수"
        android:textColor="#000000"
        android:textSize="13.75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/imageview_tel"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="23.75dp" />

    <TextView
        android:id="@+id/listview_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquarebold"
        android:text="호랑이반"
        android:textColor="#000000"
        android:textSize="13.75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/linear"
        app:layout_constraintStart_toStartOf="@id/listview_text1"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linear"
        android:layout_width="56dp"
        android:layout_height="28.5dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/listview_image1"
            android:layout_width="37dp"
            android:layout_height="21.5dp"

            app:srcCompat="@drawable/no_check" />
    </LinearLayout>




</android.support.constraint.ConstraintLayout>